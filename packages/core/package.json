{
  "name": "@versaur/core",
  "version": "0.0.1",
  "description": "Framework-agnostic CSS core for Versaur Design System",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./tokens": {
      "source": "./src/tokens/index.css",
      "import": "./dist/tokens/index.css"
    },
    "./button.css": {
      "source": "./src/components/button/button.module.css",
      "import": "./dist/components/button/button.module.css"
    },
    "./heading.css": {
      "source": "./src/components/heading/heading.module.css",
      "import": "./dist/components/heading/heading.module.css"
    },
    "./text.css": {
      "source": "./src/components/text/text.module.css",
      "import": "./dist/components/text/text.module.css"
    },
    "./avatar.css": {
      "source": "./src/components/avatar/avatar.module.css",
      "import": "./dist/components/avatar/avatar.module.css"
    },
    "./avatar-group.css": {
      "source": "./src/components/avatar-group/avatar-group.module.css",
      "import": "./dist/components/avatar-group/avatar-group.module.css"
    },
    "./hr.css": {
      "source": "./src/components/hr/hr.module.css",
      "import": "./dist/components/hr/hr.module.css"
    },
    "./tabs.css": {
      "source": "./src/components/tabs/tabs.module.css",
      "import": "./dist/components/tabs/tabs.module.css"
    },
    "./button-icon.css": {
      "source": "./src/components/button-icon/button-icon.module.css",
      "import": "./dist/components/button-icon/button-icon.module.css"
    },
    "./no-results.css": {
      "source": "./src/components/no-results/no-results.module.css",
      "import": "./dist/components/no-results/no-results.module.css"
    },
    "./page-loader.css": {
      "source": "./src/components/page-loader/page-loader.module.css",
      "import": "./dist/components/page-loader/page-loader.module.css"
    },
    "./attribute-list.css": {
      "source": "./src/components/attribute-list/attribute-list.module.css",
      "import": "./dist/components/attribute-list/attribute-list.module.css"
    },
    "./badge.css": {
      "source": "./src/components/badge/badge.module.css",
      "import": "./dist/components/badge/badge.module.css"
    },
    "./badge-group.css": {
      "source": "./src/components/badge-group/badge-group.module.css",
      "import": "./dist/components/badge-group/badge-group.module.css"
    },
    "./button-group.css": {
      "source": "./src/components/button-group/button-group.module.css",
      "import": "./dist/components/button-group/button-group.module.css"
    },
    "./card.css": {
      "source": "./src/components/card/card.module.css",
      "import": "./dist/components/card/card.module.css"
    },
    "./components/*": {
      "source": "./src/components/*/index.ts",
      "import": "./dist/components/*/index.js"
    },
    "./styles/*": {
      "source": "./src/components/*/*.module.css",
      "import": "./dist/components/*/*.module.css"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch"
  },
  "devDependencies": {
    "typescript": "^5.6.0",
    "vite": "^6.0.0",
    "vite-plugin-dts": "^4.5.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
