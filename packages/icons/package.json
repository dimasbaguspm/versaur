{
  "name": "@versaur/icons",
  "version": "0.0.1",
  "description": "SVG icon library for Versaur Design System",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "assets"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "lint": "oxlint src",
    "lint:fix": "oxlint src --fix",
    "format": "oxfmt src --write",
    "format:check": "oxfmt src --check"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.0",
    "typescript": "^5.3.3",
    "vite": "^6.0.0",
    "vite-plugin-svgr": "^4.2.0",
    "vite-plugin-dts": "^4.5.4"
  }
}
