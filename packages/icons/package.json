{
  "name": "@versaur/icons",
  "version": "1.0.0",
  "description": "SVG icon library for Versaur Design System",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dimasbaguspm/versaur.git"
  },
  "files": [
    "dist",
    "assets"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "source": "./src/index.ts",
      "import": "./dist/index.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "vite build",
    "typecheck": "tsc --noEmit",
    "dev": "vite build --watch",
    "lint": "oxlint src",
    "lint:fix": "oxlint src --fix",
    "format": "oxfmt src --write",
    "format:check": "oxfmt src --check",
    "changeset": "changeset",
    "changeset:add": "changeset add"
  },
  "devDependencies": {
    "@types/react": "^18.3.0",
    "typescript": "^5.3.3",
    "vite": "^6.0.0",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-svgr": "^4.2.0"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0"
  }
}
