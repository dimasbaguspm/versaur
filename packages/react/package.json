{
  "name": "@versaur/react",
  "version": "1.0.0",
  "description": "React components for Versaur Design System",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dimasbaguspm/versaur.git"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "exports": {
    "./primitive": {
      "types": "./dist/primitive/index.d.ts",
      "source": "./src/components/primitive/index.ts",
      "import": "./dist/primitive.js"
    },
    "./forms": {
      "types": "./dist/forms/index.d.ts",
      "source": "./src/components/forms/index.ts",
      "import": "./dist/forms.js"
    },
    "./blocks": {
      "types": "./dist/blocks/index.d.ts",
      "source": "./src/components/blocks/index.ts",
      "import": "./dist/blocks.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "source": "./src/components/utils/index.ts",
      "import": "./dist/utils.js"
    }
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "scripts": {
    "build": "vite build",
    "typecheck": "tsc --noEmit",
    "dev": "vite build --watch",
    "lint": "oxlint src",
    "lint:fix": "oxlint src --fix",
    "format": "oxfmt src --write",
    "format:check": "oxfmt src --check",
    "changeset": "changeset",
    "changeset:add": "changeset add"
  },
  "devDependencies": {
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "typescript": "^5.6.0",
    "vite": "^6.0.0",
    "vite-plugin-dts": "^4.5.4"
  },
  "peerDependencies": {
    "@versaur/core": "workspace:*",
    "@versaur/icons": "workspace:*",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  }
}
