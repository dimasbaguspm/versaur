{
  "name": "@versaur/react",
  "version": "0.0.1",
  "description": "React components for Versaur Design System",
  "files": [
    "dist"
  ],
  "type": "module",
  "exports": {
    "./primitive": {
      "types": "./dist/components/primitive/index.d.ts",
      "import": "./dist/components/primitive/index.js"
    },
    "./forms": {
      "types": "./dist/components/forms/index.d.ts",
      "import": "./dist/components/forms/index.js"
    },
    "./blocks": {
      "types": "./dist/components/blocks/index.d.ts",
      "import": "./dist/components/blocks/index.js"
    },
    "./utils": {
      "types": "./dist/components/utils/index.d.ts",
      "import": "./dist/components/utils/index.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "lint": "oxlint src",
    "lint:fix": "oxlint src --fix",
    "format": "oxfmt src --write",
    "format:check": "oxfmt src --check"
  },
  "devDependencies": {
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "typescript": "^5.6.0",
    "vite": "^6.0.0",
    "vite-plugin-dts": "^4.5.4"
  },
  "peerDependencies": {
    "@versaur/core": "workspace:*",
    "@versaur/icons": "workspace:*",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  }
}
